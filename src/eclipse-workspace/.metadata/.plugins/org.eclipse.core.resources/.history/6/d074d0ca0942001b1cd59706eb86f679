/*----------------------------------------------------------------------------------------------------------------------	
	Yıldızsız import static bildiriminin genel biçimi:
	import static <paket ismi>[.alt paket isimleri].<tür ismi>.<static eleman ismi>;
	Bu bildirim ile belirtilen static eleman ilgili java dosyasında (compilation unit) doğrudan kullanılabilir	
----------------------------------------------------------------------------------------------------------------------*/
package org.csystem.app;

import static org.csystem.util.NumberUtil.*;

import java.util.*;

class App {	
	public static void main(String [] args) 
	{
		Random r = new Random();
		
		for (int i = 0; i < 10; ++i) {
			int val = r.nextInt(100);
			
			if (isPrime(val))
				System.out.printf("%d ", val);
		}
		
		System.out.println();		
	}	
}