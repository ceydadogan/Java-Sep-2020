/*----------------------------------------------------------------------------------------------------------------------	
	Yıldızsız import static bildiriminin genel biçimi:
	import static <paket ismi>[.alt paket isimleri].<tür ismi>.<static eleman ismi>;
	
	
----------------------------------------------------------------------------------------------------------------------*/
package org.csystem.app;

import java.util.Random;

import org.csystem.util.NumberUtil;

import static org.csystem.util.NumberUtil.*;
import static java.lang.Math.*;

class App {	
	public static void main(String [] args) 
	{
		for (int i = 0; i < 100; ++i)
			if (NumberUtil.isPrime(i))
				System.out.printf("%d ", i);
		
		System.out.println();
		System.out.println(NumberUtil.isPrime(1_000_003) ? "Asal" : "Asal değil");
	}	
}